@{
    Layout = "_Layout";
    var currentuser = Context.Items["UserInfo"] as CurrentUserModel;
}
@*<script src="/js/jquery/jquery/jquery.min.js"></script>*@
<script src="~/js/select2/js/select2.js"></script>

<script src="~/js/host.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/api.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/jsApp/Statistical.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/common.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/constant.js" type="text/javascript" asp-append-version="true"></script>

<div class="main-container">
    <div class="pd-ltr-20">
        <div class="card" id="view">
            <div class="card-header bg-primary text-white" style=" font-size: 1rem; ">
                Thống kê
            </div>
            <div class="card-body card-custom">
                <form id="formView" action="javascript:void(0)">
                    <div class="col-md-12 row">
                        <div class="col-md-4 mb-3">
                            <div class="form-group row">
                                <label class="col-form-label col-lg-2" for="Wallet">
                                    Ví
                                </label>
                                <div class="col-lg-10">
                                    <select class="form-control" id="Wallet">
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 mb-3">
                            <div class="form-group row">
                                <label class="col-form-label col-lg-2" for="FromDate">
                                    Từ
                                </label>
                                <div class="col-lg-10">
                                    <input type="date" class="form-control" id="FromDate" name="FromDate">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 mb-3">
                            <div class="form-group row">
                                <label class="col-form-label col-lg-2" for="ToDate">
                                    Đến
                                </label>
                                <div class="col-lg-10">
                                    <input type="date" class="form-control" id="ToDate" name="ToDate">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-12 mb-3 btn-search-custom">
                        <div style="text-align:center; ">
                            <a href="javascript:void(0)" class="btn btn-info" onclick="onSearch()">Tìm kiếm</a>
                        </div>
                    </div>
                    @*</div>*@
                </form>
                <div class="table-responsive custom-table-scroll">
                    <table id="sellTable" class="table table-striped table-bordered zero-configuration" style="width:100%">
                        <thead class="contain-header-custom m-gray">
                            <tr>
                                <th class="sorting" style="text-align: center">STT</th>
                                <th class="sorting" style="text-align: center">ID</th>
                                <th class="sorting" style="text-align: center">Ví</th>
                                <th class="sorting" style="text-align: center">Độ hiếm</th>
                                <th class="sorting" style="text-align: center">Phân loại</th>
                                <th class="sorting" style="text-align: center">Thời gian bán</th>
                                <th class="sorting" style="text-align: center">Giá Mua (TAU)</th>
                                <th class="sorting" style="text-align: center">Giá Bán (TAU)</th>
                                <th class="sorting" style="text-align: center">Lợi Nhuận (TAU)</th>
                            </tr>
                        </thead>
                        <tbody class="contain-tbody-custom">
                        </tbody>
                    </table>
                </div>
                <div class="row">
                    <div class="col-md-7"></div>
                    <div class="col-md-5">
                        <partial name="_Pagination" />
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

<script>
    callApi_oneselect("Wallet", "Chọn ví...",
        apiConfig.api.host_user_service,
        apiConfig.api.sells.controller,
        apiConfig.api.sells.action.dropdown.path);
</script>