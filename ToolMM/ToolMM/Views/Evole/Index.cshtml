@{
    Layout = "_Layout";
    var currentuser = Context.Items["UserInfo"] as CurrentUserModel;
}
@*<script src="/js/jquery/jquery/jquery.min.js"></script>*@
<script src="~/js/select2/js/select2.js"></script>

<script src="~/js/host.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/api.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/jsApp/evoletype.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/common.js" type="text/javascript" asp-append-version="true"></script>
<script src="~/js/constant.js" type="text/javascript" asp-append-version="true"></script>


<div class="main-container">
    <div class="pd-ltr-20">
        <div class="card" id="view">
            <div class="card-header bg-primary text-white" style=" font-size: 1rem; ">
                Evole
            </div>
            <div class="card-body card-custom">
                <form id="formView" action="javascript:void(0)">
                    <div class="col-md-12 row">
                        <div class="col-md-4 mb-3">
                            <div class="form-group row">
                                <label class="col-form-label col-lg-2" for="fromD">
                                    Từ
                                </label>
                                <div class="col-lg-10">
                                    <input type="date" class="form-control" id="fromD" name="fromD">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 mb-3">
                            <div class="form-group row">
                                <label class="col-form-label col-lg-2" for="toD">
                                    Đến
                                </label>
                                <div class="col-lg-10">
                                    <input type="date" class="form-control" id="toD" name="toD">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 mb-3">
                            <div style="text-align:center; ">
                                <a href="javascript:void(0)" class="btn btn-success" onclick="ExportEvole()">Export to Excel</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 mb-3 btn-search-custom">
                        <div style="text-align:center; ">
                            <a href="javascript:void(0)" class="btn btn-info" onclick="onSearch()">Tìm kiếm</a>
                        </div>
                    </div>
                </form>
                <div class="table-responsive custom-table-scroll">
                    <table id="showListEvoleTable" class="table table-striped table-bordered zero-configuration" style="width: 100%; border: 1px solid #dee2e6 !important">
                        <thead class="contain-header-custom m-gray">
                            <tr>
                                <th class="sorting" style="text-align: center">STT</th>
                                <th class="sorting" style="text-align: center">Địa chỉ ví</th>
                                <th class="sorting" style="text-align: center">ID Hero</th>
                                <th class="sorting" style="text-align: center">Phí</th>
                                <th class="sorting" style="text-align: center">TIME</th>
                            </tr>
                        </thead>
                        <tbody class="contain-tbody-custom">
                        </tbody>
                    </table>
                </div>
                <div class="row">
                    <div class="col-md-7"></div>
                    <div class="col-md-5">
                        <partial name="_Pagination" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


